{
  "name": "esselect",
  "version": "0.0.1",
  "description": "Select JavaScript source code blocks using JSONSelect and Esprima",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "webpack --config webpack.build.js --bail",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run",
    "deploy": "gh-pages-deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yannickglt/esselect.git"
  },
  "author": "yannick Galatol",
  "license": "MIT",
  "homepage": "https://github.com/yannickglt/esselect",
  "dependencies": {
    "JSONSelect": "git://github.com/yannickglt/JSONSelect#feat/add-stringify-method",
    "angular": "^1.5.0",
    "angular-local-storage": "^0.2.2",
    "angular-ui-codemirror": "^0.3.0",
    "angular-ui-router": "^0.2.18",
    "codemirror": "^5.12.0",
    "escodegen": "^1.8.0",
    "esprima": "^2.7.2",
    "lodash": "^4.3.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.5.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "gh-pages-deploy": "^0.4.2",
    "html-webpack-plugin": "^2.7.1",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "json-loader": "^0.5.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "^0.1.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.12.1"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "clean",
      "build"
    ],
    "noprompt": true
  }
}
